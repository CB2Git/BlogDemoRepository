<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.demo.viewdraghelper"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.demo.viewdraghelper.MainActivity"
            android:label="@string/title_activity_drag_demo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.demo.viewdraghelper.DragDemoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="org.demo.viewdraghelper.SlidingActivity"
            android:label="@string/title_activity_sliding" >
        </activity>
    </application>

</manifest>